@{
    ViewBag.Title = "About";
}

<h2>@ViewBag.Title</h2>


    <div class="card mb-3" style="max-width: 540px;">
        <div class="row no-gutters">
            <div class="col-md-4">
                <img src="~/Images/journal.jfif" class="card-img" alt="journal">
